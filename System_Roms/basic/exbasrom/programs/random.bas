5 REM TEST OF RANDOM NUMBER DISTRIBUTION10 POKE &H8102,&HFF15 DIM X(9)20 GOSUB 100030 PRINT "NUMBER OF TIMES "; : INPUT A40 FOR B=0 TO 9 : X(B)=0 : NEXT B50 FOR B=1 TO ABS(A) : C=RND/10 : X(C)=X(C)+1 : NEXT B80 GOSUB 100090 PRINT TAB(10);"NUMBER";TAB(20);"TIMES"100 PRINT TAB(10);"-------";TAB(20);"-----" : PRINT110 FOR I=0 TO 9 : PRI TAB(12); : PRINT I;TAB(21);X(I)120 NEXT I130 GOSUB 1000135 R=0140 FOR J=0 TO 9 : R=R+(J*X(J)) : NEXT J150 PRINT "AVERAGE = "; R/A; " "; R-(R/A*A)155 Z=2160 IF R/A<4 THEN Z=1170 IF R/A>4 THEN Z=3180 GOSUB 1000190 ON Z GOSUB 300,400,500200 END300 PRINT "AVERAGE IS LOW" : RETURN400 PRINT "AVERAGE IS OK!!" : RETURN500 PRIN "AVERAGE IS HIGH" : RETTURN1000 PRINT : PRINT : RETURN