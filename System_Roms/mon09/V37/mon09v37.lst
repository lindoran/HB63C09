                      (     mon09v37.asm):00022         ;*
                      (     mon09v37.asm):00023         ;* Additional modifications for the HB63C09 by Dave Collins, Z80Dad
                      (     mon09v37.asm):00024         ; VERSION 3.7
                      (     mon09v37.asm):00025         ; 2024-0331:    
                      (     mon09v37.asm):00026         ;               DC      REMOVED OPTION ROM BUILDING FROM CODE FOR TINYBASIC, AND FORTH DUE TO 
                      (     mon09v37.asm):00027         ;                       SIZE CONSTRAINTS (IT IS TRIVIAL TO UNCOMMENT THEM)
                      (     mon09v37.asm):00028         ;               DC      CHANGED ADDRESS RANGES TO SUPPORT THE HB63C09
                      (     mon09v37.asm):00029         ;               DC      FIXED 'WRAP AROUND' BUG FROM ORIGINAL DUNFIELD SOURCE WHICH WOULD CAUSE
                      (     mon09v37.asm):00030         ;                       MEMORY SR TO OUTPUT FOREVER IF YOU TRIED TO DISPLAY AN ADDRESS
                      (     mon09v37.asm):00031         ;                       FROM 0xFFF0 TO THE END OF MEMORY.  THIS SLIGHTLY CHANGES THE BEHAVIOR, 
                      (     mon09v37.asm):00032         ;                       AS NOW, THE START ADDRESS WILL ALIGN WITH THE BEGINNING OF A 16 BYTE
                      (     mon09v37.asm):00033         ;                       BLOCK (STARTING AT A NUMBER WITH THE BOTTOM 4 BITS BEING CLEAR.)
                      (     mon09v37.asm):00034         ;                       SINCE THE SR ALWAYS OUTPUTS 16 BYTES THIS ALIGNS THE LAST INCREMENT OF X 
                      (     mon09v37.asm):00035         ;                       WITH ZERO SO THAT WE CAN CATCH THE REGISTER ROLLING OVER AND THE SR 
                      (     mon09v37.asm):00036         ;                       DOES NOT RUN FOREVER.  ITS ARGUABLE THIS IS THE CORRECT FUNCTIONALITY
                      (     mon09v37.asm):00037         ;                       OF THE DISPLAY MEMORY SR AS IT ALWAYS OUTPUTS 16 BYTES REGARDLESS OF 
                      (     mon09v37.asm):00038         ;                       THE LAST ADDRESS IN THE RANGE.
                      (     mon09v37.asm):00039         ;               DC      FOUND AN ISSUE WITH LOAD SR WITH THE SRECORD FORMAT.  THE SUBROTINE NEEDED 
                      (     mon09v37.asm):00040         ;                       TO IGNORE RECORD TYPE 5 - COUNT. SINCE IT IS A 16 BIT ADDRESS BUS. THE OTHER
                      (     mon09v37.asm):00041         ;                       COUNT RECORD TYPES WONT NEED TO BE TESTED FOR. (FIXED)
                      (     mon09v37.asm):00042         ;               DC      FIXED CRLF HANDLER MOSTLY. OCCASIONALLY IT WILL DUMP A SPURIOUS CR ONTO THE 
                      (     mon09v37.asm):00043         ;                       PROMPT THIS DEPENDS ON HOW THE FILE WAS TERMINATED BY THE COMPLIER. RECORD 
                      (     mon09v37.asm):00044         ;                       TYPE 9 IGNORES ANY START ADDRESS THAT IS PLACED THERE, THIS WAS ALWAYS THE 
                      (     mon09v37.asm):00045         ;                       CASE HOWEVER NOW IT READS THE CHARACTERS IN UNTIL THE END OF THE LINE.
                      (     mon09v37.asm):00046         ;               DC      SWITCHED BASIC ROM TO A MODIFIED DIGICOOLTHINGS ROM, SEE BASIC ROM SOURCE
                      (     mon09v37.asm):00047         ;                       FOR DETAILS.
                      (     mon09v37.asm):00048         ;2025-0104:                     
                      (     mon09v37.asm):00049         ;               DC      DISABLED CPU DETECTION UNTIL I CAN FIGURE OUT WHY THIS CRASHES  
                      (     mon09v37.asm):00050         ;                       WHEN SWITCHING TO NATIVE MODE. THIS MUST BE SWITCHED MANUALLY BY 
                      (     mon09v37.asm):00051         ;                       YOUR PROGRAM.
                      (     mon09v37.asm):00052         ;               DC      DISABLED ILL OP HANDLER THIS MAY BE RELATED TO THE ABOVE
                      (     mon09v37.asm):00053         ; 
                      (     mon09v37.asm):00054         ;* Additional modifications for the 6809SBC by Johnny Quest.
                      (     mon09v37.asm):00055         ;*
                      (     mon09v37.asm):00056         ;******************************************************************************
                      (     mon09v37.asm):00057         ; VERSION 3.6
                      (     mon09v37.asm):00058         ; 2021-0904:    JQ      ON ILLOP/DIV INTERRUPT, CORRECTED BUG WHERE THE HANDLER
                      (     mon09v37.asm):00059         ;                                        JUMPS TO THE BREAKPOINT DISPLAY TO EXIT BUT THE 'BRKREG'
                      (     mon09v37.asm):00060         ;                                        IS LOCATED WITHIN THE BREAKPOINT ROUTINE, WHICH CAUSES
                      (     mon09v37.asm):00061         ;                                        CONFUSIION AND EVENTUAL HANG.
                      (     mon09v37.asm):00062         ;                               JQ      SEEMS THAT DETECTION OF A 6309 AND SETTING IT TO 'NATIVE'
                      (     mon09v37.asm):00063         ;                                        MODE CAUSES PROBLEMS, SO THE MODE REGISTER IS NOT TOUCHED
                      (     mon09v37.asm):00064         ;                                        AND THE HD6309 IS LEFT IN 'EMULATION MODE'.
                      (     mon09v37.asm):00065         ;                               JQ      CORRECTED BUG IN 'CHGREG' WHERE CHANGING REGISTER 'D' WAS NOT
                      (     mon09v37.asm):00066         ;                                        RECOGNIZED AS A 16-BIT REGISTER.
                      (     mon09v37.asm):00067         ;******************************************************************************
                      (     mon09v37.asm):00068         ; VERSION 3.5
                      (     mon09v37.asm):00069         ; 2021-0824:    JQ      ON ILLOP/DIV INTERRUPT, INFORMS WHICH ONE CAUSED IT.
                      (     mon09v37.asm):00070         ;                               JQ      IF 6309 IS DETECTED, THE MODE REGISTER IS SET TO 'NATIVE'.
                      (     mon09v37.asm):00071         ; 2021-0823:    JQ      ADDED SUPPORT FOR DETECTING IF THE HOST MCU IS AN MC6809
                      (     mon09v37.asm):00072         ;                                        OR AN HD6309 AND EMITS THE PROPER SIGN-ON MESSAGE.
                      (     mon09v37.asm):00073         ;                               JQ      ADDED A BUILD DATE AND TIME TO THE SIGN-ON MESSAGE. THE
                      (     mon09v37.asm):00074         ;                                        LINUX "build.sh" SCRIPT CREATES A SEPARATE ASSEMBLY FILE
                      (     mon09v37.asm):00075         ;                                        THAT IS "INCLUDED" IN THE MAIN ASSEMBLY FILE.
                      (     mon09v37.asm):00076         ;                               JQ      BE AWARE THAT THE DISASSEMBLER DOES NOT YET SUPPORT THE
                      (     mon09v37.asm):00077         ;                                        6309 EXTENDED INSTRUCTIONS.
                      (     mon09v37.asm):00078         ; 2021-0419:    JQ      ADDED SUPPORT FOR MERGING THE BINARY IMAGES OF THE
                      (     mon09v37.asm):00079         ;                                        LANGUAGE SUPPORT MODULES DIRECTLY INTO THE MON09
                      (     mon09v37.asm):00080         ;                                        IMAGE FILE. USE CONDITIONAL ASSEMBLY TO ENABLE/DISABLE.
                      (     mon09v37.asm):00081         ;                                        CONSTRUCTED MASTER BUILD SCRIPT TO ASSEMBLE THE VARIOUS
                      (     mon09v37.asm):00082         ;                                        LANGUAGE MODULES INTO A MASTER FIRMWARE IMAGE FILE. THUS,
                      (     mon09v37.asm):00083         ;                                        INCLUDING THEM INTO THE MON09 IMAGE AS DATA STATEMENTS
                      (     mon09v37.asm):00084         ;                                        IS UNNESSESARY.
                      (     mon09v37.asm):00085         ; 2021-0412:    JQ      CHANGED THE MANNER IN WHICH NMI IS HANDLED SO THAT THE
                      (     mon09v37.asm):00086         ;                                        VECTOR CAN BE SET IN MEMORY JUST AS THE FIRQ, IRQ, SWI2
                      (     mon09v37.asm):00087         ;                                        AND SWI3 HANDLERS DO. IF AN NMI VECTOR IS NOT SPECIFIED,
                      (     mon09v37.asm):00088         ;                                        MON09 DEFAULTS TO THE INTERNAL NMI HANDLER, WHICH IS SHOWN
                      (     mon09v37.asm):00089         ;                                        BY THE 'DV' COMMAND. ADDED ADDITIONAL BYTES TO THE DATA
                      (     mon09v37.asm):00090         ;                                        TABLES AND AN NMI HANDLER. USE CONDITIONAL STATEMENT TO
                      (     mon09v37.asm):00091         ;                                        ENABLE IT.
                      (     mon09v37.asm):00092         ;                               JQ      ADDED HD6309 ILLOP INSTRUCTION HANDLER. IF NO VECTOR
                      (     mon09v37.asm):00093         ;                                        SPECIFIED, DEFAULTS TO THE MON09 ILLOP HANDLER. USE
                      (     mon09v37.asm):00094         ;                                        CONDITIONAL STATEMENT TO ENABLE IT.
                      (     mon09v37.asm):00095         ;                               JQ      ADDED 'JUMP' COMMAND TO ACCESS THE FP BASIC, TINY BASIC
                      (     mon09v37.asm):00096         ;                                        AND FORTH MODULES IN ROM. USE CONDITIONAL STATEMENT TO
                      (     mon09v37.asm):00097         ;                                        ENABLE IT.
                      (     mon09v37.asm):00098         ;                               JQ      ADDED JUMP TABLE OF 'HOOKS' THAT PROVIDES A SUMMARY OF
                      (     mon09v37.asm):00099         ;                                        USER ACCESSIBLE MON09 ROM ENTRY ADDRESSES AND IRQ RE-
                      (     mon09v37.asm):00100         ;                                        VECTORING ADDRESSES IN DATA RAM. IT IS INTENDED TO BE
                      (     mon09v37.asm):00101         ;                                        EASILY IDENTIFIED AND EXTRACTED FROM THE SYMBOL TABLE
                      (     mon09v37.asm):00102         ;                                        TO BE 'INCLUDED' IN THE USER'S PROGRAM FILE. SINCE THE
                      (     mon09v37.asm):00103         ;                                        TABLE IS LOCATED AT
                      (     mon09v37.asm):00104         ;                               JQ      IN 'FILMEM' ROUTINE, ADDED CODE TO LIMIT THE USER
                      (     mon09v37.asm):00105         ;                                        SPECIFIED ENDING ADDRESS SO AS NOT TO OVERWRITE MON09
                      (     mon09v37.asm):00106         ;                                        DATA IN RAM. THIS EFFECTIVELY LIMITS THE 'FM' COMMAND
                      (     mon09v37.asm):00107         ;                                        TO RAM ONLY ON THE 6809SBC.
                      (     mon09v37.asm):00108         ;
