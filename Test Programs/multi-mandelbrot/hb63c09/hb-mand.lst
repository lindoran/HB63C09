                      (      hb-mand.asm):00133                 INCLUDE "bn2bcd.asm"    ;bin 2 bcd converter
                      (       bn2bcd.asm):00001         ;
                      (       bn2bcd.asm):00002         ; Title:                Binary to BCD Conversion 
                      (       bn2bcd.asm):00003         ;
                      (       bn2bcd.asm):00004         ; Name:                 BN2BCD
                      (       bn2bcd.asm):00005         ;
                      (       bn2bcd.asm):00006         ; Purpose:              Converts 16 bit Binary Number's 
                      (       bn2bcd.asm):00007         ;                       To 24 bit Packed BCD
                      (       bn2bcd.asm):00008         ; Entry:                Register D = Binary data
                      (       bn2bcd.asm):00009         ; Exit:                 Register Q = BCD data
                      (       bn2bcd.asm):00010         ;                       F = tens and ones 
                      (       bn2bcd.asm):00011         ;                       E = thousands and one-hundreds
                      (       bn2bcd.asm):00012         ;                       B = ten-thousands
                      (       bn2bcd.asm):00013         
                      (       bn2bcd.asm):00014         ;
                      (       bn2bcd.asm):00015         ; Registers Used:       Q,CC
                      (       bn2bcd.asm):00016         ; 
                      (       bn2bcd.asm):00017         
109F                  (       bn2bcd.asm):00018         BN2BCD:
                      (       bn2bcd.asm):00019         ; Allocate space on stack for 1 byte to store 100's digit
109F 327D             (       bn2bcd.asm):00020                 LEAS    -3,S            ; Allocate 3 bytes on stack
10A1 6FE4             (       bn2bcd.asm):00021                 CLR     ,S              ; Clear space for 10000's digit
10A3 6F61             (       bn2bcd.asm):00022                 CLR     1,S             ; Clear space for 1000's digit
10A5 6F62             (       bn2bcd.asm):00023                 CLR     2,S             ; Clear Space for 100's digit
                      (       bn2bcd.asm):00024         
                      (       bn2bcd.asm):00025         ; Calculate the 10000's digit
                      (       bn2bcd.asm):00026         ; Devide data by 10000
10A7 11C6FF           (       bn2bcd.asm):00027                 LDF     #$FF            ; Start quotient at -1
10AA                  (       bn2bcd.asm):00028         D10000LP:
10AA 115C             (       bn2bcd.asm):00029                 INCF                    ; add 1 to quotient
10AC 832710           (       bn2bcd.asm):00030                 SUBD    #10000          ; subtract 10000 from the dividend (D)
10AF 24F9             (       bn2bcd.asm):00031                 BCC     D10000LP        ; jump if difference is still positive
10B1 C32710           (       bn2bcd.asm):00032                 ADDD    #10000          ; if not add the last 10000 back.
10B4 11E7E4           (       bn2bcd.asm):00033                 STF     ,S              ; save the 10000's digit in stack space S
                      (       bn2bcd.asm):00034         ; Calculate the 1000's digit
                      (       bn2bcd.asm):00035         ; Divide the data by 1000
10B7 11C6FF           (       bn2bcd.asm):00036                 LDF     #$FF            ; Start quotient at -1
10BA                  (       bn2bcd.asm):00037         D1000LP:
10BA 115C             (       bn2bcd.asm):00038                 INCF                    ; add 1 to quotient
10BC 8303E8           (       bn2bcd.asm):00039                 SUBD    #1000           ; subtract 1000 from the dividend (D)
10BF 24F9             (       bn2bcd.asm):00040                 BCC     D1000LP         ; jump if difference is still postive
10C1 C303E8           (       bn2bcd.asm):00041                 ADDD    #1000           ; if not add the last 1000 back.
10C4 11E761           (       bn2bcd.asm):00042                 STF     1,S             ; save the 1000's digit in stack space 1,S
                      (       bn2bcd.asm):00043         
                      (       bn2bcd.asm):00044         ; Calculate 100's digit (for numbers greater than or equal to 100)
                      (       bn2bcd.asm):00045         ; Divide the data by 100        
10C7 11C6FF           (       bn2bcd.asm):00046                 LDF     #$FF            ; Start quotient at -1
10CA                  (       bn2bcd.asm):00047         D100LP:
10CA 115C             (       bn2bcd.asm):00048                 INCF                    ; Add 1 to quotient
10CC 830064           (       bn2bcd.asm):00049                 SUBD    #100            ; Subtract 100 from the dividend (D)
10CF 24F9             (       bn2bcd.asm):00050                 BCC     D100LP          ; Jump if difference is still positive
10D1 C30064           (       bn2bcd.asm):00051                 ADDD    #100            ; If not, add the last 100 back
10D4 11E762           (       bn2bcd.asm):00052                 STF     2,S             ; Save 100's digit in stack space 2,S
                      (       bn2bcd.asm):00053         
                      (       bn2bcd.asm):00054         ; Calculate 10's digit from the remainder of 100's division
                      (       bn2bcd.asm):00055         ; Divide the remainder by 10
10D7 11C6FF           (       bn2bcd.asm):00056                 LDF     #$FF            ; Start quotient at -1
10DA 115C             (       bn2bcd.asm):00057         D10LP:  INCF                    ; Add 1 to quotient
10DC 83000A           (       bn2bcd.asm):00058                 SUBD    #10             ; Subtract 10 from the dividend (D)
10DF 24F9             (       bn2bcd.asm):00059                 BCC     D10LP           ; Jump if difference is still positive
10E1 C3000A           (       bn2bcd.asm):00060                 ADDD    #10             ; If not, add the last 10 back
                      (       bn2bcd.asm):00061         
                      (       bn2bcd.asm):00062         ; Combine the 1's and 10's digits
                      (       bn2bcd.asm):00063         ; Shift the 10's digit to the high nibble (left shift 4 times)  
10E4 1FF8             (       bn2bcd.asm):00064                 TFR     F,A             ; Load 10's digit in A
10E6 48               (       bn2bcd.asm):00065                 LSLA                    ; Shift 1 time (equivalent to multiplying by 2)
10E7 48               (       bn2bcd.asm):00066                 LSLA                    ; Shift 2 times (equivalent to multiplying by 4)
10E8 48               (       bn2bcd.asm):00067                 LSLA                    ; Shift 3 times (equivalent to multiplying by 8)
10E9 48               (       bn2bcd.asm):00068                 LSLA                    ; Shift 4 times (equivalent to multiplying by 16)
10EA 103089           (       bn2bcd.asm):00069                 ADDR    A,B             ; add 1's digit to 10's digit, result in B (6309)
10ED 1F06             (       bn2bcd.asm):00070                 TFR     D,W             ; move lower digits to bottom of Q
10EF E662             (       bn2bcd.asm):00071                 LDB     2,S             ; load 100's digit into B
10F1 A661             (       bn2bcd.asm):00072                 LDA     1,S             ; load 1000's digit into A
10F3 48               (       bn2bcd.asm):00073                 LSLA                    ; Shift 1 time (equivalent to multiplying by 2)
10F4 48               (       bn2bcd.asm):00074                 LSLA                    ; Shift 2 times (equivalent to multiplying by 4)
10F5 48               (       bn2bcd.asm):00075                 LSLA                    ; Shift 3 times (equivalent to multiplying by 8)
10F6 48               (       bn2bcd.asm):00076                 LSLA                    ; Shift 4 times (equivalent to multiplying by 16)
10F7 103089           (       bn2bcd.asm):00077                 ADDR    A,B             ; add 100's and 1000's digit, result in B (6309)
10FA 1F9E             (       bn2bcd.asm):00078                 TFR     B,E             ; move B into E (100's and 1000's)
                      (       bn2bcd.asm):00079         
                      (       bn2bcd.asm):00080         ; Return the final BCD value (A = BCD data)
10FC 14               (       bn2bcd.asm):00081                 SEXW                    ; CLEAR W
10FD E6E4             (       bn2bcd.asm):00082                 LDB     ,S              ; Load 10000's into B
10FF 3263             (       bn2bcd.asm):00083                 LEAS    3,S             ; Deallocate 3 bytes from stack
1101 39               (       bn2bcd.asm):00084                 RTS                     ; Return with BCD data in register A
                      (       bn2bcd.asm):00085         
